{
  "name": "Lab L-9 User Authentication App",
  "description": "Lab L-9: Implementing User Authentication in a Blockchain Application",
  "version": "1.0.0",
  "moduleType": "package",
  "author": "Rob",
  "frontend": {
    "packagePath": "frontend"
  },
  "docker": {
    "services": {
      "authserver": {
        "packagePath": "backend",
        "port": 3000,
        "protocol": "http",
        "domain": "localhost"
      }
